<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".View.Welcome_View">


    <ImageView
        android:id="@+id/ceviche"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/ceviche01"
        />

    <ImageView
        android:id="@+id/trazado"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/a_trazado"
         />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="295dp"
        android:layout_height="295dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="@+id/trazado"
        app:layout_constraintEnd_toEndOf="@+id/trazado"
        app:layout_constraintStart_toStartOf="@+id/trazado"
        app:layout_constraintTop_toTopOf="@+id/trazado" />

</android.support.constraint.ConstraintLayout>